<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.warehouse.services.persistence.dao.CheckMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.warehouse.services.persistence.model.Check">
		<id column="id" property="id" />
		<result column="check_code" property="checkCode" />
		<result column="check_time" property="checkTime" />
		<result column="finish_time" property="finishTime" />
		<result column="begin_time" property="beginTime" />
		<result column="warehouse_id" property="warehouseId" />
		<result column="profit_lost" property="profitLost" />
		<result column="check_note" property="checkNote" />
		<result column="transaction_by" property="transactionBy" />
		<result column="originator_id" property="originatorId" />
		<result column="originator_name" property="originatorName" />
		<result column="status" property="status" />
		<result column="field1" property="field1" />
		<result column="field2" property="field2" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, check_code AS checkCode, check_time AS checkTime, finish_time AS finishTime, begin_time AS beginTime, warehouse_id AS warehouseId, profit_lost AS profitLost, check_note AS checkNote, transaction_by AS transactionBy, originator_id AS originatorId, originator_name AS originatorName, status, field1, field2
    </sql>

</mapper>
