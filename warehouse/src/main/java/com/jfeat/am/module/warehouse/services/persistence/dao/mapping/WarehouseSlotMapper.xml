<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.warehouse.services.persistence.dao.WarehouseSlotMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.warehouse.services.persistence.model.WarehouseSlot">
		<id column="id" property="id" />
		<result column="slot_code" property="slotCode" />
		<result column="slot_name" property="slotName" />
		<result column="warehouse_id" property="warehouseId" />
		<result column="slot_note" property="slotNote" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, slot_code AS slotCode, slot_name AS slotName, warehouse_id AS warehouseId, slot_note AS slotNote
    </sql>

</mapper>
