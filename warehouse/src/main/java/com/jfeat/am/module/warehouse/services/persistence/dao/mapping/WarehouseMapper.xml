<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.warehouse.services.persistence.dao.WarehouseMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.warehouse.services.persistence.model.Warehouse">
		<id column="id" property="id" />
		<result column="warehouse_code" property="warehouseCode" />
		<result column="warehouse_name" property="warehouseName" />
		<result column="warehouse_PCD" property="warehousePCD" />
		<result column="warehouse_address" property="warehouseAddress" />
		<result column="warehouse_charger" property="warehouseCharger" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, warehouse_code AS warehouseCode, warehouse_name AS warehouseName, warehouse_PCD AS warehousePCD, warehouse_address AS warehouseAddress, warehouse_charger AS warehouseCharger
    </sql>

</mapper>
