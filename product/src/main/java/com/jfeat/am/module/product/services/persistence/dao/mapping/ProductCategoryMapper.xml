<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.product.services.persistence.dao.ProductCategoryMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.product.services.persistence.model.ProductCategory">
		<id column="id" property="id" />
		<result column="org_id" property="orgId" />
		<result column="org_tag" property="orgTag" />
		<result column="category_name" property="categoryName" />
		<result column="category_code" property="categoryCode" />
		<result column="pid" property="pid" />
		<result column="category_description" property="categoryDescription" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, org_id AS orgId, org_tag AS orgTag, category_name AS categoryName,category_code AS categoryCode, pid, category_description AS categoryDescription
    </sql>

</mapper>
