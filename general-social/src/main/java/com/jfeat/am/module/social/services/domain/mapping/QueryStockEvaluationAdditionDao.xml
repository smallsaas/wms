<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.social.services.domain.dao.QueryStockEvaluationAdditionDao">
    <sql id="Base_Column_List">
            se_stock_evaluation_addition.*
        </sql>
        <select id="findStockEvaluationAdditionPage" resultType="StockEvaluationAddition" parameterType="StockEvaluationAddition">
            SELECT se_stock_evaluation_addition.*
            FROM se_stock_evaluation_addition
            WHERE 1=1
                             <if test="id != null and id>0 ">
                 AND se_stock_evaluation_addition.id LIKE CONCAT('%',#{id},'%')
            </if>
                                             <if test="evaluateId != null and evaluateId>0 ">
                 AND se_stock_evaluation_addition.evaluate_id LIKE CONCAT('%',#{evaluateId},'%')
            </if>
                             <if test="note != null and note!= ''">
                 AND se_stock_evaluation_addition.note LIKE CONCAT('%',#{note},'%')
            </if>
                                             <if test="createTime != null and createTime>0 ">
                 AND se_stock_evaluation_addition.create_time LIKE CONCAT('%',#{createTime},'%')
            </if>
                    </select>
</mapper>