<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.social.services.domain.dao.QueryMemberReportDao">
    <sql id="Base_Column_List">
            so_member_report.*
        </sql>
        <select id="findMemberReportPage" resultType="MemberReport" parameterType="MemberReport">
            SELECT so_member_report.*
            FROM so_member_report
            WHERE 1=1
                             <if test="id != null and id>0 ">
                 AND so_member_report.id LIKE CONCAT('%',#{id},'%')
            </if>
                                             <if test="memberId != null and memberId>0 ">
                 AND so_member_report.member_id LIKE CONCAT('%',#{memberId},'%')
            </if>
                                             <if test="referenceId != null and referenceId>0 ">
                 AND so_member_report.reference_id LIKE CONCAT('%',#{referenceId},'%')
            </if>
                                             <if test="causeId != null and causeId!= ''">
                 AND so_member_report.cause_id LIKE CONCAT('%',#{causeId},'%')
            </if>
                             <if test="desc != null and desc!= ''">
                 AND so_member_report.desc LIKE CONCAT('%',#{desc},'%')
            </if>
                                             <if test="createTime != null and createTime>0 ">
                 AND so_member_report.create_time LIKE CONCAT('%',#{createTime},'%')
            </if>
                                             <if test="dealResult != null and dealResult!= ''">
                 AND so_member_report.deal_result LIKE CONCAT('%',#{dealResult},'%')
            </if>
                                             <if test="dealTime != null and dealTime>0 ">
                 AND so_member_report.deal_time LIKE CONCAT('%',#{dealTime},'%')
            </if>
                    </select>
</mapper>