<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.social.services.domain.dao.QueryStockCollectDao">
    <sql id="Base_Column_List">
            se_stock_collect.*
        </sql>
        <select id="findStockCollectPage" resultType="StockCollect" parameterType="StockCollect">
            SELECT se_stock_collect.*
            FROM se_stock_collect
            WHERE 1=1
                             <if test="id != null and id>0 ">
                 AND se_stock_collect.id LIKE CONCAT('%',#{id},'%')
            </if>
                                             <if test="memberId != null and memberId>0 ">
                 AND se_stock_collect.member_id LIKE CONCAT('%',#{memberId},'%')
            </if>
                                             <if test="activityId != null and activityId>0 ">
                 AND se_stock_collect.activity_id LIKE CONCAT('%',#{activityId},'%')
            </if>
                                             <if test="createTime != null and createTime>0 ">
                 AND se_stock_collect.create_time LIKE CONCAT('%',#{createTime},'%')
            </if>
                    </select>
</mapper>