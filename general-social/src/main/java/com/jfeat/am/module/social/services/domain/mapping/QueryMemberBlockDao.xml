<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.social.services.domain.dao.QueryMemberBlockDao">
    <sql id="Base_Column_List">
            so_member_block.*
        </sql>
        <select id="findMemberBlockPage" resultType="MemberBlock" parameterType="MemberBlock">
            SELECT so_member_block.*
            FROM so_member_block
            WHERE 1=1
                             <if test="id != null and id>0 ">
                 AND so_member_block.id LIKE CONCAT('%',#{id},'%')
            </if>
                                             <if test="memberId != null and memberId>0 ">
                 AND so_member_block.member_id LIKE CONCAT('%',#{memberId},'%')
            </if>
                                             <if test="referenceId != null and referenceId>0 ">
                 AND so_member_block.reference_id LIKE CONCAT('%',#{referenceId},'%')
            </if>
                                             <if test="createTime != null and createTime>0 ">
                 AND so_member_block.create_time LIKE CONCAT('%',#{createTime},'%')
            </if>
                             <if test="cause != null and cause!= ''">
                 AND so_member_block.cause LIKE CONCAT('%',#{cause},'%')
            </if>
                    </select>
</mapper>