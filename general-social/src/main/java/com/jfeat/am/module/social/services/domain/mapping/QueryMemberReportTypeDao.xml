<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.social.services.domain.dao.QueryMemberReportTypeDao">
    <sql id="Base_Column_List">
            so_member_report_type.*
        </sql>
        <select id="findMemberReportTypePage" resultType="MemberReportType" parameterType="MemberReportType">
            SELECT so_member_report_type.*
            FROM so_member_report_type
            WHERE 1=1
                             <if test="id != null and id>0 ">
                 AND so_member_report_type.id LIKE CONCAT('%',#{id},'%')
            </if>
                             <if test="name != null and name!= ''">
                 AND so_member_report_type.name LIKE CONCAT('%',#{name},'%')
            </if>
                             <if test="pid != null and pid>0 ">
                 AND so_member_report_type.pid LIKE CONCAT('%',#{pid},'%')
            </if>
                    </select>
</mapper>