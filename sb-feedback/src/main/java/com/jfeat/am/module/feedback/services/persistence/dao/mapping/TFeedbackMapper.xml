<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.feedback.services.persistence.dao.TFeedbackMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.feedback.services.persistence.model.TFeedback">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="content" property="content" />
		<result column="created_date" property="createdDate" />
		<result column="unread" property="unread" />
		<result column="solved" property="solved" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id AS userId, content, created_date AS createdDate, unread, solved
    </sql>

</mapper>
