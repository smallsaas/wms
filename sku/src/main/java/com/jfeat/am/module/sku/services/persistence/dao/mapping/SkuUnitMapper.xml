<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.sku.services.persistence.dao.SkuUnitMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.sku.services.persistence.model.SkuUnit">
		<id column="id" property="id" />
		<result column="unit_name" property="unitName" />
		<result column="is_primary" property="isPrimary" />
		<result column="sku_id" property="skuId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, unit_name AS unitName, is_primary AS isPrimary, sku_id AS skuId
    </sql>

</mapper>
