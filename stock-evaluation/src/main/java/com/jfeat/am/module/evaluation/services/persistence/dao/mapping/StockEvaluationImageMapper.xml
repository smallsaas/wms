<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.evaluation.services.persistence.dao.StockEvaluationImageMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.evaluation.services.persistence.model.StockEvaluationImage">
		<id column="id" property="id" />
		<result column="evaluate_id" property="evaluateId" />
		<result column="image" property="image" />
		<result column="status" property="status" />
		<result column="create_time" property="createTime" />
		<result column="evaluation_addition_id" property="evaluationAdditionId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, evaluate_id AS evaluateId, image, status, create_time AS createTime, evaluation_addition_id AS evaluationAdditionId
    </sql>

</mapper>
