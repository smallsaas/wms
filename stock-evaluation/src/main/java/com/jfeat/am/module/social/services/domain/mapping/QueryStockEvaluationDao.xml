<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.social.services.domain.dao.QueryStockEvaluationDao">
    <sql id="Base_Column_List">
            se_stock_evaluation.*
        </sql>
        <select id="findStockEvaluationPage" resultType="StockEvaluation" parameterType="StockEvaluation">
            SELECT se_stock_evaluation.*
            FROM se_stock_evaluation
            WHERE 1=1
                             <if test="id != null and id>0 ">
                 AND se_stock_evaluation.id LIKE CONCAT('%',#{id},'%')
            </if>
                                             <if test="stockId != null and stockId>0 ">
                 AND se_stock_evaluation.stock_id LIKE CONCAT('%',#{stockId},'%')
            </if>
                                             <if test="memberId != null and memberId>0 ">
                 AND se_stock_evaluation.member_id LIKE CONCAT('%',#{memberId},'%')
            </if>
                             <if test="note != null and note!= ''">
                 AND se_stock_evaluation.note LIKE CONCAT('%',#{note},'%')
            </if>
                                             <if test="createTime != null and createTime>0 ">
                 AND se_stock_evaluation.create_time LIKE CONCAT('%',#{createTime},'%')
            </if>
                    </select>
</mapper>