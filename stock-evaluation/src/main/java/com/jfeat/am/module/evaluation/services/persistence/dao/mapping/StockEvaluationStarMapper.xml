<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.evaluation.services.persistence.dao.StockEvaluationStarMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.evaluation.services.persistence.model.StockEvaluationStar">
		<id column="id" property="id" />
		<result column="evaluate_id" property="evaluateId" />
		<result column="star_name" property="starName" />
		<result column="star_value" property="starValue" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, evaluate_id AS evaluateId, star_name AS starName, star_value AS starValue
    </sql>

</mapper>
