<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.score.services.persistence.dao.ScoreMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.score.services.persistence.model.Score">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="type" property="type" />
		<result column="score" property="score" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id AS userId, type, score, update_time AS updateTime
    </sql>

</mapper>
