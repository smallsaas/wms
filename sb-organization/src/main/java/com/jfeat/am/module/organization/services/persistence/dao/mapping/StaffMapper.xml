		<result column="work_age" property="workAge" />
		<result column="hire_date" property="hireDate" />
		<result column="leave_date" property="leaveDate" />
		<result column="status" property="status" />
		<result column="profile_id" property="profileId" />
		<result column="position_id" property="positionId" />
		<result column="create_time" property="createTime" />
		<result column="note" property="note" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dept_id AS deptId, work_age AS workAge, hire_date AS hireDate, leave_date AS leaveDate, status, profile_id AS profileId, position_id AS positionId, create_time AS createTime, note
    </sql>

</mapper>
