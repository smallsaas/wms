<mapper namespace="com.jfeat.am.module.organization.services.persistence.dao.StaffMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.organization.services.persistence.model.Staff">
		<id column="id" property="id" />
